<h1> All of the Users </h1>



  <div class="users">
    <% @users.each do |user| %>
      <% unless user == current_user %>
        <%= link_to user.username, user_path(user)  %>
        <%= link_to 'Delete', admin_user_path(user), method: :delete %>
      <% end %>
      </br>
    <% end %>
  </div>
