<%= form_for [@country, @city, @review] do |f| %>
<div>
  <%= f.label :food_rating%>
  <br>
  <%= f.select :food_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :nightlife_rating%>
  <br>
  <%= f.select :nightlife_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :landmark_rating%>
  <br>
  <%= f.select :landmark_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :cost_rating %>
  <br>
  <%= f.select :cost_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :weather_rating %>
  <br>
  <%= f.select :weather_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :culture_rating %>
  <br>
  <%= f.select :culture_rating, @review.star_selections %>
</div>
<div>
  <%= f.label :description %>
  <br>
  <%= f.text_area :description, size: "40x10" %>
</div>
<div>
  <%= f.submit  "Submit"%>
</div>
<% end %>
